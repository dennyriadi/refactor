{
  "cacheDirectory": "<rootDir>/tmp/tests_cache",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "server/**/*.{js}"
  ],
  "coverageReporters": [
    "json",
    "lcov",
    "text"
  ],
  "testEnvironment": "node",
  "verbose": true,
  "unmockedModulePathPatterns": [
    "<rootDir>/node_modules"
  ]
}
